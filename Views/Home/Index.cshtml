@model IEnumerable<Post>

@{ 

}

<h2>Home Page!</h2>
<img src="~/content/static/img/DSC6393.jpg" width="500" />
<div>
    <ul>
        @foreach (var post in Model) { 
        <li>
            <a asp-controller="Home" asp-action="Post" asp-route-id="@post.Id">@post.Title - @post.Created</a>
        </li>
        }
    </ul>
</div>

